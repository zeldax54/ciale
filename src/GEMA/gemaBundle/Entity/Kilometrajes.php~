namespace GEMA\gemaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Symfony\Component\Validator\Constraints as Assert;
use Symfony\Bridge\Doctrine\Validator\Constraints\UniqueEntity;

/**
 * Kilometrajes
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="GEMA\gemaBundle\Entity\KilometrajesRepository")
 */
class Kilometrajes
{

    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\ManyToOne(targetEntity="GEMA\gemaBundle\Entity\Vehiculo",inversedBy="id", cascade={"persist"})
     */
    private $vehiculo;

    /**
     * @var float
     *
     * @ORM\Column(name="actualKilometros", type="float")
     */
    private $actualKilometros;

}