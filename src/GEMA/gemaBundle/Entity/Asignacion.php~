<?php

namespace GEMA\gemaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Symfony\Component\Validator\Constraints as Assert;
use Symfony\Bridge\Doctrine\Validator\Constraints\UniqueEntity;

/**
 * Area
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="GEMA\gemaBundle\Entity\AsignacionRepository")
 */
class Asignacion
{

    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;


    /**
     * @ORM\ManyToOne(targetEntity="GEMA\gemaBundle\Entity\TipoAsignacion" , inversedBy="id")
     */

    private $tipoasignacion;

    /**
     * @ORM\ManyToOne(targetEntity="GEMA\gemaBundle\Entity\TarjetaMagnetica" , inversedBy="id")
     */

    private $tarjeta;

    /**
     * @var string
     * @Assert\NotBlank()
     * @Assert\Length(min = 1, max = 10)
     * @ORM\Column(name="litros", type="float", length=5)
     */
    private $litros;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="mesyear", type="datetime")
     */
    private $mesyear;



}
